<configuration name="xml_cdr.conf" description="XML CDR CURL logger">
<settings>
   <param name="url" value="http://localhost/cdr.php"/>
   <param name="retries" value="2"/>
   <param name="delay" value="120"/>
   <param name="log-dir" value="/var/tmp/cdr"/>
   <param name="err-log-dir" value="/var/tmp/cdr/errors"/>
   <param name="encode" value="True"/>
</settings>
</configuration>
